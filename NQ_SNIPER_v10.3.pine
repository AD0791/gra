ENABLE_ALL = input.bool(false, "ğŸ“ All Sessions", group="ğŸŒ Sessions")

isLondon() => hour(time, "America/New_York") >= 3 and hour(time, "America/New_York") < 5
isNY() => 
    h = hour(time, "America/New_York")
    m = minute(time, "America/New_York")
    (h == 9 and m >= 30) or h == 10 or (h == 11 and m < 30)

isActiveSession() => ENABLE_ALL or (ENABLE_LONDON and isLondon()) or (ENABLE_NY and isNY())

